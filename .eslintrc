{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "comma-dangle": [
      "off"
    ],
    "jsx-a11y/no-static-element-interactions": [
      "off"
    ],
    "react/prop-types": [
      "off"
    ],
    "class-methods-use-this": [
      "off"
    ],
    "no-return-assign": [
      "off"
    ],
    "import/no-extraneous-dependencies": [
      "off"
    ]
  },
  "globals": {
    "document": true
  }
}